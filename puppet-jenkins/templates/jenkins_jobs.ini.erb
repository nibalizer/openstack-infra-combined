[jenkins]
user=<%= @username %>
password=<%= @password %>
url=<%= @url %>
query_plugins_info=<%= @query_plugins_info %>

<% @extensions.each do |extension| -%>
["<%= extension['name'] %>"]
<% extension['parameters'].each do |parameter| -%>
<%= parameter['name'] %> = <%= parameter['value'] %>
<% end -%>
<% end -%>
