[access "refs/heads/*"]
abandon = group app-catalog-core
label-Code-Review = -2..+2 group app-catalog-core
label-Verified = -1..+1 group app-catalog-ci
label-Workflow = -1..+1 group app-catalog-core

[access "refs/tags/*"]
