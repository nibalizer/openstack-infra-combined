[access "refs/heads/*"]
abandon = group defcore-core
label-Code-Review = -2..+2 group defcore-core
label-Workflow = -1..+1 group defcore-core

[access "refs/tags/*"]
pushSignedTag = group defcore-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
