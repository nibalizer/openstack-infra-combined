[access "refs/heads/*"]
abandon = group kiloeyes-core
label-Code-Review = -2..+2 group kiloeyes-core
label-Workflow = -1..+1 group kiloeyes-core

[access "refs/tags/*"]
pushSignedTag = group kiloeyes-core

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
