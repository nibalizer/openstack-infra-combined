[access "refs/heads/*"]
abandon = group grenade-core
label-Code-Review = -2..+2 group grenade-core
label-Workflow = -1..+1 group grenade-core

[access "refs/tags/*"]
pushSignedTag = group grenade-release

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
