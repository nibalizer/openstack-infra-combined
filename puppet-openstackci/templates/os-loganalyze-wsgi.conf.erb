[general]
filter = SevFilter
view = HTMLView
file_conditions = /etc/os_loganalyze/file_conditions.yaml
generate_folder_index = true

<% if @swift_authurl != "" -%>
[swift]
authurl=<%= @swift_authurl %>
user=<%= @swift_user %>
password=<%= @swift_key %>
tenant=<%= @swift_tenant_name %>
region=<%= @swift_region_name %>
chunk_size=64
container=<%= @swift_default_container %>
<% end -%>
